{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["src/**/__tests__/*", "src/**/*.spec.ts", "src/**/*.test.ts", "env.d.ts"],
  "compilerOptions": {
    "composite": true,
